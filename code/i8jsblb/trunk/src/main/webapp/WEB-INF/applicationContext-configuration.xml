            <value>com.inov8.microbank.common.model.ActionStatusModel</value>
            <value>com.inov8.microbank.common.model.AppUserModel</value>
            <value>com.inov8.microbank.common.model.portal.transactiondetailmodule.ExtendedTransactionAllViewModel</value>
            <value>com.inov8.microbank.common.model.P2PDetailModel</value>
