  <bean id="xmlMarshaller" class="com.inov8.microbank.server.service.xml.XmlMarshallerImpl">
  	<property name="marshaller" ref="jaxb2Marshaller"/>
  	<property name="unmarshaller" ref="jaxb2Marshaller"/>
  </bean>
  <bean id="popupParser" class="com.inov8.framework.common.util.PopupParser" init-method="initialize">
     <property name="popupXmlFile" value="classpath:/popups.xml"/>
  </bean>
  <aop:aspectj-autoproxy/>
</beans>

