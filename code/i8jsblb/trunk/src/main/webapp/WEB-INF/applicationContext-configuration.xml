  <bean id="messageSource" class="org.springframework.context.support.ResourceBundleMessageSource">
    <property name="basenames">
      <list>        
        <value>server-config</value><!-- Top file = High priority -->
        <value>ApplicationResources</value>
        <value>report_queries</value>
        <value>FieldMapping</value>
      </list>
    </property>

  </bean>
  
 <bean id="baseTransactionProxy" class="org.springframework.transaction.interceptor.TransactionProxyFactoryBean" abstract="true">
    <property name="transactionManager">
      <ref bean="springTransactionManager"/>
    </property>
    <property name="transactionAttributes">
      <props>
        <prop key="get*">PROPAGATION_REQUIRED,readOnly</prop>
        <prop key="find*">PROPAGATION_REQUIRED,readOnly</prop>
        <prop key="search*">PROPAGATION_REQUIRED,readOnly</prop>
        <prop key="load*">PROPAGATION_REQUIRED,readOnly</prop>
        <prop key="retrieve*">PROPAGATION_REQUIRED,readOnly</prop>
        <prop key="list*">PROPAGATION_REQUIRED,readOnly</prop>
        <prop key="is*">PROPAGATION_REQUIRED,readOnly</prop>
        <prop key="create*">PROPAGATION_REQUIRED, -FrameworkCheckedException</prop>
        <prop key="insert*">PROPAGATION_REQUIRED, -FrameworkCheckedException</prop>
        <prop key="define*">PROPAGATION_REQUIRED, -FrameworkCheckedException</prop>
        <prop key="save*">PROPAGATION_REQUIRED, -FrameworkCheckedException</prop>
        <prop key="new*">PROPAGATION_REQUIRED, -FrameworkCheckedException</prop>
        <prop key="make*">PROPAGATION_REQUIRED, -FrameworkCheckedException</prop>
        <prop key="delete*">PROPAGATION_REQUIRED, -FrameworkCheckedException</prop>
        <prop key="remove*">PROPAGATION_REQUIRED, -FrameworkCheckedException</prop>
        <prop key="eliminate*">PROPAGATION_REQUIRED, -FrameworkCheckedException</prop>
        <prop key="update*">PROPAGATION_REQUIRED, -FrameworkCheckedException</prop>
        <prop key="edit*">PROPAGATION_REQUIRED, -FrameworkCheckedException</prop>
        <prop key="modify*">PROPAGATION_REQUIRED, -FrameworkCheckedException</prop>
        <prop key="change*">PROPAGATION_REQUIRED, -FrameworkCheckedException</prop>
        <prop key="merge*">PROPAGATION_REQUIRED, -FrameworkCheckedException</prop>
        <prop key="integrate*">PROPAGATION_REQUIRED, -FrameworkCheckedException</prop>
        <prop key="deactivate*">PROPAGATION_REQUIRED, -FrameworkCheckedException</prop>
        <prop key="activate*">PROPAGATION_REQUIRED, -FrameworkCheckedException</prop>
        <prop key="reactivate*">PROPAGATION_REQUIRED, -FrameworkCheckedException</prop>
        <prop key="workflow*">PROPAGATION_REQUIRED, -FrameworkCheckedException, -WorkFlowException</prop>
        <prop key="publish*">PROPAGATION_REQUIRED, -FrameworkCheckedException</prop>
        <prop key="produce*">PROPAGATION_REQUIRED, -FrameworkCheckedException</prop>
        <prop key="*RequiresNewTransaction">PROPAGATION_REQUIRES_NEW, -FrameworkCheckedException</prop>
        <prop key="getBillInfo">PROPAGATION_REQUIRED, -FrameworkCheckedException</prop>
        <prop key="check*">PROPAGATION_REQUIRED,readOnly, -FrameworkCheckedException</prop>
        <prop key="billPayment*">PROPAGATION_REQUIRED, readOnly,  -FrameworkCheckedException</prop>
        <prop key="rollback">PROPAGATION_REQUIRES_NEW, -FrameworkCheckedException</prop>                
        <prop key="reverseOrphanTransaction*">PROPAGATION_REQUIRED, -FrameworkCheckedException, -WorkFlowException</prop>                
      </props>
    </property>
  </bean>
  
  <bean id="frameworkExceptionTranslator" class="com.inov8.framework.common.exception.FrameworkExceptionTranslator">
    <property name="messageSource">
      <ref local="messageSource"/>
    </property>
  </bean>
  
  <bean id="messageUtil" class="com.inov8.microbank.common.util.MessageUtil">
    <property name="messageSource">
      <ref local="messageSource"/>
    </property>
  </bean>
  
  
  
   <bean id="reflectionUtils" class="com.inov8.framework.common.util.ReflectionUtils">
    <property name="messageSource">
      <ref local="messageSource"/>
    </property>
  </bean>
  
  <bean id="mailEngine" class="com.inov8.framework.server.service.common.MailEngine">
    <property name="mailSender" ref="mailSender"/>
    <property name="velocityEngine" ref="velocityEngine"/>
  </bean>
  <bean id="mailSender" class="org.springframework.mail.javamail.JavaMailSenderImpl">
    <property name="host" value="${mail.host}"/>
    <property name="username" value="${mail.username}"/>
    <property name="password" value="${mail.password}"/>
  </bean>
  <!-- Configure Velocity for sending e-mail -->
  <bean id="velocityEngine" class="org.springframework.ui.velocity.VelocityEngineFactoryBean">
    <property name="velocityProperties">
      <props>
        <prop key="resource.loader">class</prop>
        <prop key="class.resource.loader.class">org.apache.velocity.runtime.resource.loader.ClasspathResourceLoader        </prop>
        <prop key="velocimacro.library">        </prop>
      </props>
    </property>
  </bean>
  <bean id="mailMessage" class="org.springframework.mail.SimpleMailMessage">
    <property name="from" value="${mail.default.from}"/>
  </bean>
