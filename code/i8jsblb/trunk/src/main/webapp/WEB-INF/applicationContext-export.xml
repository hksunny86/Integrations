<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="oracleJdbcDao" class="com.inov8.microbank.export.dao.jdbc.OracleJdbcDaoImpl" autowire="byName"/>

    <bean id="exportZipManager" parent="baseTransactionProxy">
        <property name="target">
            <bean class="com.inov8.microbank.export.service.ExportZipManagerImpl" autowire="byName">
                <property name="oracleJdbcDao" ref="oracleJdbcDao"/>
            </bean>
        </property>
    </bean>

</beans>